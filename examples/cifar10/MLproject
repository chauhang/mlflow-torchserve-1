name: CIFAR10-captum-insights-example

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      max_epochs: {type: int, default: 5}
      lr: {type: float, default: 0.01}

    command: |
          python cifar10_classifier.py \
            --max_epochs {max_epochs} \
            --lr {lr}
